{
  "name": "demo",
  "version": "0.1.0",
  "description": "A simple dapp for Aptos Snap testing.",
  "engines": {
    "node": ">= 12.0.0"
  },
  "scripts": {
    "lint": "yarn lint:eslint",
    "deploy": "./deploy.sh",
    "lint:style:fix": "yarn lint:eslint --fix",
    "lint:eslint": "eslint . --cache --ext js,ts",
    "start": "NODE_ENV=local webpack-dev-server",
    "build": "webpack --mode production",
    "test": "yarn lint"
  },
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/KeystoneHQ/aptossnap.git"
  },
  "files": [
    "dist/"
  ],
  "dependencies": {
    "@keystonehq/aptossnap-adapter": "^0.2.2",
    "aptos": "^1.3.12",
    "stream-browserify": "^3.0.0",
    "tweetnacl": "^1.0.3"
  },
  "devDependencies": {
    "@metamask/eslint-config": "^6.0.0",
    "@metamask/eslint-config-nodejs": "^6.0.0",
    "clean-webpack-plugin": "^4.0.0",
    "compression-webpack-plugin": "^10.0.0",
    "copy-webpack-plugin": "^6.0.2",
    "dotenv-webpack": "^8.0.1",
    "eslint": "^7.29.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.4.0",
    "gh-pages": "^3.1.0",
    "prettier": "^2.3.1",
    "process": "^0.11.10",
    "webpack": "5.74.0",
    "webpack-cli": "^4.10.0",
    "webpack-dev-server": "^4.11.0"
  }
}
