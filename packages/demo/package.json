{
  "name": "demo",
  "version": "5.2.0",
  "description": "A simple dapp used in MetaMask e2e tests.",
  "engines": {
    "node": ">= 12.0.0"
  },
  "scripts": {
    "lint": "yarn run lint:style && yarn run lint:types",
    "lint:types": "tsc --noEmit --pretty",
    "lint:style": "eslint --color 'src/**/*.{js,ts,tsx}'",
    "lint:style:fix": "yarn run lint:style --fix",
    "start": "webpack-dev-server",
    "build": "webpack --mode production",
    "test": "yarn lint"
  },
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/KeystoneHQ/aptossnap.git"
  },
  "files": [
    "dist/"
  ],
  "dependencies": {
    "@keystonehq/aptossnap-adapter": "*",
    "aptos": "^1.3.10",
    "stream-browserify": "^3.0.0"
  },
  "devDependencies": {
    "clean-webpack-plugin": "^3.0.0",
    "compression-webpack-plugin": "^10.0.0",
    "copy-webpack-plugin": "^6.0.2",
    "webpack": "5.74.0",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "3.11.1"
  }
}
